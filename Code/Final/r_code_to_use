tess = read.csv("C:/Users/kforr/Downloads/PS_2025.12.16_18.04.27.csv", header = TRUE, skip = 99)
habitable = read.csv("C:/Users/kforr/Downloads/hwc_table_all.csv", header = TRUE)
names = habitable$Name
tess$pl_name[tess$pl_name %in% names]
names
non_habitable = tess$pl_name[tess$pl_name %in% names == FALSE]
length(non_habitable)
tess_non_habitable = tess[tess$pl_name %in% non_habitable,]
tess_habitable = tess[tess$pl_name %in% non_habitable,]
#summary(tess_non_habitable)
#summary(tess_habitable)
summary(habitable)
full_dataset = read.csv("C:/Users/kforr/Downloads/PS_2025.12.18_11.22.50.csv", header = TRUE, skip = 96)
confirmed_habitable = full_dataset[full_dataset$pl_name %in% habitable,]
#summary(confirmed_habitable)
confirmed_habitable$pl_name
#add a column indicating whether the planet is habitable
full_dataset$habitable = c(1:length(full_dataset$habitable))
full_dataset$habitable = full_dataset$pl_name %in% names
#drop some columns we don't need
full_dataset = subset(full_dataset, select = -c(pl_pubdate, releasedate, rowupdate))
full_dataset = subset(full_dataset, select = -c(pl_radj, pl_radjerr1, pl_radjerr2, pl_radjlim)) #redundant data, just in different units
full_dataset = subset(full_dataset, select = -c(pl_bmassj, pl_bmassjerr1, pl_bmassjerr2, pl_bmassjlim)) #redundant data, just in different units
full_dataset = subset(full_dataset, select = -c(disc_year, disc_facility))
#remove error data from dataset but store somewhere else in case we need them
error_terms = subset(full_dataset, select = c(pl_orbpererr1, pl_orbpererr2, pl_orbperlim, pl_orbsmaxerr1, pl_orbsmaxerr2, pl_orbsmaxlim,
pl_radeerr1, pl_radeerr2, pl_radelim, pl_bmasseerr1, pl_bmasseerr2, pl_bmasselim, pl_orbeccenerr1, pl_orbeccenerr2, pl_orbeccenlim,
pl_insolerr1, pl_insolerr2, pl_insollim, pl_eqterr1, pl_eqterr2, pl_eqtlim, st_tefferr1, st_tefferr2, st_tefflim, st_raderr1, st_raderr2, st_radlim,
st_masserr1, st_masserr2, st_masslim, st_meterr1, st_meterr2, st_metlim, st_loggerr1, st_loggerr2, st_logglim, sy_disterr1, sy_disterr2,
sy_vmagerr1, sy_vmagerr2, sy_kmagerr1, sy_kmagerr2, sy_gaiamagerr1, sy_gaiamagerr2))
full_dataset = subset(full_dataset, select = -c(pl_orbpererr1, pl_orbpererr2, pl_orbperlim, pl_orbsmaxerr1, pl_orbsmaxerr2, pl_orbsmaxlim,
pl_radeerr1, pl_radeerr2, pl_radelim, pl_bmasseerr1, pl_bmasseerr2, pl_bmasselim, pl_orbeccenerr1, pl_orbeccenerr2, pl_orbeccenlim,
pl_insolerr1, pl_insolerr2, pl_insollim, pl_eqterr1, pl_eqterr2, pl_eqtlim, st_tefferr1, st_tefferr2, st_tefflim, st_raderr1, st_raderr2, st_radlim,
st_masserr1, st_masserr2, st_masslim, st_meterr1, st_meterr2, st_metlim, st_loggerr1, st_loggerr2, st_logglim, sy_disterr1, sy_disterr2,
sy_vmagerr1, sy_vmagerr2, sy_kmagerr1, sy_kmagerr2, sy_gaiamagerr1, sy_gaiamagerr2))
